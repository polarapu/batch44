git init
ls -la
git config --global user.name "Prasad"
git config --global user.email "prasad@gmail.com" 
git config --list


GIT Phases:-
---------------
1) Workspace
2) Staging /index area
3) Local repo

touch file-name
touch file1
git status
git add file1
git status
git commit -m "first commit" file1
git status
git log
git show cid

touch file4 file5 file6
git status
git add file4 file5 or
git add . or
git add * or 
git add -A

git status
git commit -m "label" file1  (single file)
git commit -m "label" 

git add file1  (workspace to staging/index)
git reset HEAD file  (staging/index to workspace)

git commit -m "label"  (staging to local)
git reset --soft cid   (local to staging)

git reset --mixed cid  (local to workspace)
or
git reset HEAD~1

git reset --hard cid


git log
git log --oneline
git log -3
git log --oneline -4
git log --author=prasad
git log --since=yy-mm-dd
or
git log --after="2018-12-07"
git log --befor="2019-02-07"
or
git log --until=yy-mm-dd

git log --after="yesterday"

git log --since=yy-mm-dd --until=yy-mm-dd
or
git log --after=yy-mm-dd --before=yy-mm-dd

git log --grep="msg"
git log -- file-name
git log --after="2019-06-28 20:26" --before="2019-06-29 20:30"


git clone https://github.com/polarapu/batch44.git

ls
cd batch44

touch prasad
git add .
git commit -m "Test commit"
git push